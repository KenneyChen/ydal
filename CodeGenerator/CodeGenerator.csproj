<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Version>2.0.3</Version>
    <PackageId>YDal.CodeGenerator</PackageId>
    <Authors>KenneyChen</Authors>
    <Company>KenneyChen</Company>
    <Product>KenneyChen</Product>
    <Description>ydal框架代码生成器：https://github.com/KenneyChen/ydal</Description>
    <PackageProjectUrl>https://github.com/KenneyChen/ydal</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>ydal框架代码生成器：https://github.com/KenneyChen/ydal</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="CodeTemplate\IRepository.txt" />
    <None Remove="CodeTemplate\IService.txt" />
    <None Remove="CodeTemplate\Model.txt" />
    <None Remove="CodeTemplate\Repository.txt" />
    <None Remove="CodeTemplate\Service.txt" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="CodeTemplate\IRepository.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="CodeTemplate\IService.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="CodeTemplate\Model.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="CodeTemplate\Repository.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="CodeTemplate\Service.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Dapper" Version="2.0.35" />
        <PackageReference Include="EntityFramework" Version="6.4.4" />
        <PackageReference Include="Microsoft.EntityFrameworkCore" Version="3.1.9" />
        <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="3.1.9" />
        <PackageReference Include="Microsoft.Extensions.Configuration" Version="3.1.9" />
        <!--<PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="3.1.9" />-->
        <!--<PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="3.1.9" />-->
        <PackageReference Include="Microsoft.Extensions.Configuration.FileExtensions" Version="3.1.9" />
        <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="3.1.9" />
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="3.1.9" />
        <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="3.1.9" />
        <PackageReference Include="Microsoft.Extensions.Logging" Version="3.1.9" />
        <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="3.1.9" />
        <!--<PackageReference Include="MSTest.TestFramework" Version="2.1.0" />-->
        <PackageReference Include="MySql.Data.EntityFrameworkCore" Version="8.0.22" />
        <PackageReference Include="System.Data.SqlClient" Version="4.8.2" />
        <!--<PackageReference Include="xunit.extensibility.core" Version="2.4.1" />-->
        <PackageReference Include="Z.EntityFramework.Extensions.EFCore" Version="3.0.101" />
    </ItemGroup>
</Project>
